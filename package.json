  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:no-watch": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src",
    "prettier": "npx prettier './src/**/*.+(js|jsx|ts|tsx|html|css|json|md)'",
